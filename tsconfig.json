{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@/ui/*": ["src/components/ui/*"],
      "@root/*": [
        "./*"
      ]
    }
  },
  "include": [
    ".astro/types.d.ts",
    "**/*",
    "./src/types/*.type.ts"
  ],
  "exclude": [
    "dist"
  ]
}
